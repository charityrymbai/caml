{
  "name": "app",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "cd apps/backend && npx prisma generate && cd ../.. && turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "migrate_db": "cd apps/backend && npx prisma migrate dev",
    "generate_db": "cd apps/backend && npx prisma generate"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.2.3",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
